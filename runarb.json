{
    "name":"arb_file_example.py",
    "tool-version": "1.0",
    "description": "example of handling arbitrary files",
    "command-line":"arb_file_example.py [FILE_LIST] [OUTPUT_PREFIX]",
    "schema-version":"0.5",
    "container-image": {
	"type":"docker",
	"image":"shots47s/arb_file_test"
    },
    "inputs": [
	{
	    "id":"files",
	    "name":"Input File List",
	    "value-key": "[FILE_LIST]",
	    "type": "File",
	    "list": true,
	    "optional": false,
            "command-line-flag":"--files"
	},
	{
	    "id":"output",
	    "name":"Output File",
            "value-key": "[OUTPUT_PREFIX]",
	    "type": "String",
	    "optional": false,
            "command-line-flag":"--out"
	}
    ],
    "output-files":[
	{
	    "id":"output_pre",
	    "name":"Output File",
	    "path-template":"[OUTPUT_PREFIX].log"
	}
    ]
}
	
